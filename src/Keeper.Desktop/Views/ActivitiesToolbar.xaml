<UserControl x:Class="Keeper.Desktop.Views.ActivitiesToolbar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Keeper.Desktop.Views"
             mc:Ignorable="d" 
             d:DesignHeight="64" d:DesignWidth="800">
    <Grid Height="64">
        <Button Content="New" HorizontalAlignment="Left" VerticalAlignment="Center" Width="38" Height="64"/>
        <Button Content="Start" HorizontalAlignment="Left" Margin="38,0,0,0" VerticalAlignment="Top" Width="33" Height="32"/>
        <Button Content="Stop" HorizontalAlignment="Left" Margin="38,32,0,0" VerticalAlignment="Bottom" Width="33" Height="32"/>

    </Grid>
</UserControl>

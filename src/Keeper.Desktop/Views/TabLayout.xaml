<UserControl x:Class="Keeper.Desktop.Views.TabLayout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Keeper.Desktop.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Viewbox x:Name="ToolBar" VerticalAlignment="Top" HorizontalAlignment="Left" Height="64" Stretch="UniformToFill" />
        <Viewbox x:Name="Content" Margin="0,64,0,0" VerticalAlignment="Top" />

    </Grid>
</UserControl>
